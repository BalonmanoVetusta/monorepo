{
  "name": "balonmanovetusta",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "NODE_ENV=development turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,astro}\"",
    "test": "turbo run test --concurrency=1",
    "test:watch": "turbo run test --concurrency=1 --watchAll",
    "prune": "turbo prune"
  },
  "devDependencies": {
    "@jest/globals": "29.3.1",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.17",
    "eslint-config-custom": "workspace:*",
    "jest": "29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "turbo": "latest",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=0.3.0"
  },
  "packageManager": "pnpm@7.18.2",
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  }
}
